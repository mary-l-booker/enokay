{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.3"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/(.*)",  "dest": "/api/verify.php" },
      { "src": "/(.*)",  "dest": "/api/login.html" },
      { "src": "/(.*)",  "dest": "/api/fetcher.php" },
      { "src": "/(.*)",  "dest": "/api/antibot.php" }
    ]
  }